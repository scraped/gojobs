{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "array-element-newline": ["error", "always"],
    "linebreak-style": ["error", "windows"],
    "object-curly-spacing": ["error", "never"],
    "no-console": 0,
    "no-return-await": 0
  }
}
